{
    "$schema": "http://json.schemastore.org/swcrc",
    "minify": true,
    "jsc": {
        "minify": {
            "compress": {
                "unused": true
            },
            "mangle": true
        }
    },
    "module": {
        "type": "commonjs",
        "strict": false,
        "strictMode": true,
        "lazy": false,
        "noInterop": false
    },
    "sourceMaps": true
}